{
  "$schema": "file:///C:/Users/darwj1/Documents/Ironveil/Nebula/dist/schemas/ServerMetaSchema.schema.json",
  "meta": {
    "version": "1.0.0",
    "name": "Ironveil RP (Minecraft 1.20.4)",
